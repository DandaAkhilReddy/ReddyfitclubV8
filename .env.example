# =============================================================================
# ReddyFit Club V8 - Environment Variables Template
# =============================================================================
# IMPORTANT: Copy this file to .env and fill in your actual credentials
# NEVER commit .env to version control (it's in .gitignore)
# =============================================================================

# -----------------------------------------------------------------------------
# Firebase Configuration (HIPAA-Compliant)
# -----------------------------------------------------------------------------
# Obtain from: https://console.firebase.google.com/project/YOUR_PROJECT/settings/general
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID

# Firebase Admin SDK (Server-side only, NOT in frontend)
FIREBASE_ADMIN_PROJECT_ID=your_project_id
FIREBASE_ADMIN_CLIENT_EMAIL=firebase-adminsdk@your_project.iam.gserviceaccount.com
FIREBASE_ADMIN_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY\n-----END PRIVATE KEY-----\n"

# -----------------------------------------------------------------------------
# Google OAuth 2.0 (For Sign-In)
# -----------------------------------------------------------------------------
# Obtain from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# -----------------------------------------------------------------------------
# Azure Configuration
# -----------------------------------------------------------------------------
# Azure Subscription & Tenant
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_TENANT_ID=your_azure_tenant_id
AZURE_CLIENT_ID=your_azure_app_client_id
AZURE_CLIENT_SECRET=your_azure_app_client_secret

# Azure Resource Group
AZURE_RESOURCE_GROUP=reddyfit-club-v8-rg
AZURE_LOCATION=eastus

# -----------------------------------------------------------------------------
# Azure OpenAI (GPT-4o Vision for Body Composition Analysis)
# -----------------------------------------------------------------------------
# Obtain from: https://portal.azure.com -> Azure OpenAI -> Keys and Endpoint
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o-vision  # Your deployment name
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Azure OpenAI Model Configuration
AZURE_OPENAI_MODEL=gpt-4o
AZURE_OPENAI_MAX_TOKENS=4096
AZURE_OPENAI_TEMPERATURE=0.7

# -----------------------------------------------------------------------------
# Azure Blob Storage (Encrypted Photo Storage)
# -----------------------------------------------------------------------------
# Obtain from: Azure Portal -> Storage Account -> Access Keys
AZURE_STORAGE_ACCOUNT_NAME=reddyfitclubv8storage
AZURE_STORAGE_ACCOUNT_KEY=your_storage_account_key
AZURE_STORAGE_CONNECTION_STRING="DefaultEndpointsProtocol=https;AccountName=your_account;AccountKey=your_key;EndpointSuffix=core.windows.net"

# Blob Container Names
AZURE_BLOB_CONTAINER_SCANS=body-scans
AZURE_BLOB_CONTAINER_PROFILES=profile-photos

# -----------------------------------------------------------------------------
# Azure Key Vault (Secrets Management)
# -----------------------------------------------------------------------------
AZURE_KEY_VAULT_NAME=reddyfit-club-v8-kv
AZURE_KEY_VAULT_URI=https://reddyfit-club-v8-kv.vault.azure.net/

# -----------------------------------------------------------------------------
# Azure Functions (API Backend)
# -----------------------------------------------------------------------------
AZURE_FUNCTIONS_APP_NAME=reddyfit-club-v8-api
AZURE_FUNCTIONS_URL=https://reddyfit-club-v8-api.azurewebsites.net

# -----------------------------------------------------------------------------
# Azure Application Insights (Monitoring)
# -----------------------------------------------------------------------------
APPINSIGHTS_INSTRUMENTATIONKEY=your_app_insights_instrumentation_key
APPLICATIONINSIGHTS_CONNECTION_STRING="InstrumentationKey=your_key;IngestionEndpoint=https://eastus-8.in.applicationinsights.azure.com/;LiveEndpoint=https://eastus.livediagnostics.monitor.azure.com/"

# -----------------------------------------------------------------------------
# Microsoft Semantic Kernel (D65 Licenses for Multi-Agent)
# -----------------------------------------------------------------------------
# D65 License Key (if applicable - check with Microsoft)
SEMANTIC_KERNEL_LICENSE_KEY=your_d65_license_key
SEMANTIC_KERNEL_VERSION=1.0.0

# Semantic Kernel Agent Configuration
SEMANTIC_KERNEL_MAX_RETRIES=3
SEMANTIC_KERNEL_TIMEOUT_MS=60000

# -----------------------------------------------------------------------------
# Temporal.io Cloud (Workflow Orchestration)
# -----------------------------------------------------------------------------
# Obtain from: https://cloud.temporal.io/namespaces
TEMPORAL_ADDRESS=ap-northeast-1.aws.api.temporal.io:7233
TEMPORAL_NAMESPACE=your_namespace.tjhly
TEMPORAL_API_KEY=your_temporal_api_key
TEMPORAL_TLS_ENABLED=true

# Temporal Task Queues
TEMPORAL_TASK_QUEUE_SCAN_ANALYSIS=body-fat-analysis-queue
TEMPORAL_TASK_QUEUE_NOTIFICATIONS=notifications-queue

# Temporal Client Configuration
TEMPORAL_CLIENT_TIMEOUT_MS=30000
TEMPORAL_WORKFLOW_TIMEOUT_MS=600000  # 10 minutes

# -----------------------------------------------------------------------------
# Supermemory (Infinite Context for AI Agents)
# -----------------------------------------------------------------------------
# Obtain from: https://overlap.ai/api-keys
VITE_SUPERMEMORY_API_KEY=sm_your_supermemory_api_key
VITE_SUPERMEMORY_BASE_URL=https://api.supermemory.ai/
SUPERMEMORY_ENCRYPTION_ENABLED=true

# Supermemory Namespaces
SUPERMEMORY_NAMESPACE_ESTIMATION=estimation_history
SUPERMEMORY_NAMESPACE_NUTRITION=nutrition_preferences
SUPERMEMORY_NAMESPACE_WORKOUT=workout_history
SUPERMEMORY_NAMESPACE_GOALS=user_goals

# -----------------------------------------------------------------------------
# CodeRabbit (Automated Code Reviews)
# -----------------------------------------------------------------------------
# Obtain from: https://coderabbit.ai/settings/api-keys
CODERABBIT_API_KEY=cr-your_coderabbit_api_key

# -----------------------------------------------------------------------------
# JIRA (Project Management)
# -----------------------------------------------------------------------------
# JIRA Configuration
JIRA_URL=http://localhost:8080
JIRA_USER=admin
JIRA_TOKEN=your_jira_api_token
JIRA_PROJECT_KEY=RFIT

# -----------------------------------------------------------------------------
# SendGrid (Email Notifications - HIPAA BAA Required)
# -----------------------------------------------------------------------------
# Obtain from: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=SG.your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@reddyfit.com
SENDGRID_FROM_NAME=ReddyFit Club

# SendGrid Template IDs
SENDGRID_TEMPLATE_SCAN_READY=d-scan_ready_template_id
SENDGRID_TEMPLATE_GOAL_ACHIEVED=d-goal_achieved_template_id
SENDGRID_TEMPLATE_BREACH_NOTIFICATION=d-breach_notification_template_id

# -----------------------------------------------------------------------------
# Google Gemini AI (Alternative/Backup to Azure OpenAI)
# -----------------------------------------------------------------------------
# Obtain from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-pro-vision
GEMINI_MAX_OUTPUT_TOKENS=2048

# -----------------------------------------------------------------------------
# Application Configuration
# -----------------------------------------------------------------------------
# Environment
NODE_ENV=development  # development | staging | production
VITE_APP_ENV=development

# Application URLs
VITE_APP_URL=http://localhost:5173
VITE_API_BASE_URL=http://localhost:7071  # Azure Functions local

# Frontend Configuration
VITE_APP_NAME=ReddyFit Club V8
VITE_APP_VERSION=1.0.0

# -----------------------------------------------------------------------------
# HIPAA Compliance Configuration
# -----------------------------------------------------------------------------
# Audit Logging
AUDIT_LOG_RETENTION_YEARS=7
AUDIT_LOG_ENABLED=true

# Session Timeout (15 minutes idle for HIPAA)
SESSION_TIMEOUT_MINUTES=15
SESSION_ABSOLUTE_TIMEOUT_HOURS=8

# MFA Configuration
MFA_REQUIRED=true
MFA_TRUST_DEVICE_DAYS=30

# Data Encryption
ENCRYPTION_ALGORITHM=AES-256-GCM
ENCRYPTION_KEY_ROTATION_DAYS=90

# Breach Notification
BREACH_NOTIFICATION_THRESHOLD=500  # Number of individuals
BREACH_NOTIFICATION_TIMELINE_DAYS=60

# -----------------------------------------------------------------------------
# Feature Flags
# -----------------------------------------------------------------------------
FEATURE_FLAG_GOOGLE_LOGIN=true
FEATURE_FLAG_AZURE_AD_LOGIN=false
FEATURE_FLAG_MFA=true
FEATURE_FLAG_EXPORT_DATA=true
FEATURE_FLAG_DELETE_ACCOUNT=true
FEATURE_FLAG_SCAN_COMPARISON=true
FEATURE_FLAG_NOTIFICATIONS=true

# -----------------------------------------------------------------------------
# Development & Testing
# -----------------------------------------------------------------------------
# Logging
LOG_LEVEL=debug  # debug | info | warn | error
VITE_ENABLE_CONSOLE_LOGGING=true

# Testing
ENABLE_TEST_MODE=false
TEST_USER_EMAIL=test@reddyfit.com

# Mock Services (for development without external dependencies)
MOCK_AZURE_OPENAI=false
MOCK_TEMPORAL=false
MOCK_SUPERMEMORY=false
MOCK_FIREBASE=false

# -----------------------------------------------------------------------------
# Rate Limiting & Performance
# -----------------------------------------------------------------------------
# API Rate Limits
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_HOUR=1000

# Azure OpenAI Rate Limits
AZURE_OPENAI_REQUESTS_PER_MINUTE=120
AZURE_OPENAI_TOKENS_PER_MINUTE=40000

# Image Upload Limits
MAX_IMAGE_SIZE_MB=5
MAX_IMAGES_PER_SCAN=4

# -----------------------------------------------------------------------------
# Monitoring & Alerting
# -----------------------------------------------------------------------------
# Alert Thresholds
ALERT_WORKFLOW_FAILURE_RATE=0.05  # 5%
ALERT_API_RESPONSE_TIME_MS=3000
ALERT_ERROR_RATE=0.01  # 1%

# Health Check
HEALTH_CHECK_INTERVAL_SECONDS=30

# -----------------------------------------------------------------------------
# Notes
# -----------------------------------------------------------------------------
# 1. NEVER commit this file with actual credentials to version control
# 2. Use Azure Key Vault in production to manage secrets
# 3. Ensure all BAAs are signed before using third-party services in production
# 4. Rotate credentials every 90 days (HIPAA requirement)
# 5. Use environment-specific .env files (.env.development, .env.staging, .env.production)
